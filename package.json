{
  "name": "el-mediador-de-sofia",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "jest --config jest.config.mjs",
    "test:coverage": "jest --config jest.config.mjs --coverage",
    "test:watch": "jest --config jest.config.mjs --watch",
    "guardian": "node scripts/run-guardian.mjs --mode=audit",
    "guardian:audit": "node scripts/run-guardian.mjs --mode=audit",
    "guardian:repair": "node scripts/run-guardian.mjs --mode=repair",
    "guardian:full": "node scripts/run-guardian.mjs --mode=full --auto-fix",
    "garden:frontend": "node scripts/run-frontend-gardener.mjs",
    "garden:backend": "dotnet run --project backend/BackendAgents/",
    "garden:all": "npm run garden:frontend && npm run garden:backend",
    "data:garden": "node scripts/run-data-gardener.mjs --mode=audit",
    "data:garden:full": "node scripts/run-data-gardener.mjs --mode=full --auto-fix",
    "ops:gardener": "tsx scripts/run-ops-gardener.ts",
    "ops:gardener:audit": "tsx scripts/run-ops-gardener.ts audit",
    "ops:gardener:repair": "tsx scripts/run-ops-gardener.ts repair --auto-fix",
    "ops:gardener:full": "tsx scripts/run-ops-gardener.ts full --auto-fix",
    "ops:ritual": "tsx scripts/run-ritual.ts",
    "ops:watch": "tsx scripts/run-night-watch.ts",
    "ops:activate": "tsx scripts/activate-night-protocol.ts",
    "ops:assign": "tsx scripts/assign-flow-owners.ts"
  },
  "dependencies": {
    "@sentry/react": "^10.38.0",
    "@sentry/tracing": "^7.120.4",
    "@sentry/vite-plugin": "^4.9.1",
    "@supabase/supabase-js": "^2.39.7",
    "@tanstack/react-query": "^5.28.0",
    "axios": "^1.13.5",
    "chart.js": "^4.5.1",
    "cors": "^2.8.6",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.4",
    "express": "^5.2.1",
    "jspdf": "^4.1.0",
    "lucide-react": "^0.344.0",
    "multer": "^2.0.2",
    "puppeteer": "^24.37.2",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.13.0",
    "recharts": "^2.15.4",
    "sonner": "^1.4.3",
    "xlsx": "^0.18.5",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^25.2.3",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.24",
    "jest": "^29.7.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19",
    "ts-jest": "^29.4.6",
    "tsx": "^4.21.0",
    "typescript": "^5.2.2",
    "vite": "^7.3.1"
  }
}
