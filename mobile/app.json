{
  "expo": {
    "name": "Serendipity Mobile",
    "slug": "serendipity-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    // "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    // "splash": {
    //   "image": "./assets/splash.png",
    //   "resizeMode": "contain",
    //   "backgroundColor": "#0f172a"
    // },
    "ios": {
      "supportsTabletMode": true,
      "bundleIdentifier": "com.campanerasanti.serendipity"
    },
    "android": {
      // "adaptiveIcon": {
      //   "foregroundImage": "./assets/adaptive-icon.png",
      //   "backgroundColor": "#0f172a"
      // },
      "package": "com.campanerasanti.serendipity"
    },
    // "web": {
    //   "favicon": "./assets/favicon.png"
    // },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 21
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ]
    ],
    "scheme": "serendipity",
    "experiments": {
      "tsconfigPaths": true,
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "serendipity-mobile-prod"
      }
    }
  }
}
